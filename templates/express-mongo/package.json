{
  "name": "base-app",
  "version": "1.0.0",
  "description": "A base web app template for production with express.js and mongoose following airbnb js style guide.",
  "engines": {
    "node": "8.11.3"
  },
  "main": "server.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint src",
    "start": "node server.js",
    "dev": "nodemon -r dotenv/config server.js",
    "test": "mocha",
    "local-test": "mocha -r dotenv/config",
    "test-coverage": "nyc --reporter=text --reporter=html mocha --timeout=3000"
  },
  "keywords": [
    "Express",
    "MongoDb",
    "Mongoose",
    "Production",
    "Airbnb"
  ],
  "author": "Your Name Here",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "express": "4.16.3",
    "fs-extra": "7.0.0",
    "helmet": "3.12.1",
    "http-errors": "1.6.3",
    "moment": "2.22.2",
    "mongoose": "5.0.17",
    "morgan": "1.9.0",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "mocha": "5.1.1",
    "nodemon": "1.17.4",
    "nyc": "11.7.2",
    "supertest": "3.0.0"
  }
}
