{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": ["airbnb-base"],
  "plugins": ["import"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "comma-dangle": ["error", "only-multiline"]
  }
}
